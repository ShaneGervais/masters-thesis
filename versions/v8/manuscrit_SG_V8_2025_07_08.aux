\@writefile{lof}{\contentsline {figure}{\numberline {22}{\ignorespaces Résultats de l'amplitude de probabilité pour le trajet de polarisation $\ket {H} \rightarrow \ket {R} \rightarrow \ket {V} \rightarrow \ket {L}$. Les données sont calculées à partir de la partie réelle de la valeur faible par les équations \ref {eq:amplitude_a} et \ref {eq:amplitude_b}. Les courbes théoriques provenant de les amplitudes de probabilités de l'équation \ref {eq:expval_t_2} soit $\sqrt  {|cos^2(\theta )|}$ pour $a$ et $\sqrt  {|sin^2(\theta )|}$ pour $b$.}}{87}{figure.caption.22}\protected@file@percent }
\newlabel{fig:HRVL_prob_amp}{{22}{87}{Résultats de l'amplitude de probabilité pour le trajet de polarisation $\ket {H} \rightarrow \ket {R} \rightarrow \ket {V} \rightarrow \ket {L}$. Les données sont calculées à partir de la partie réelle de la valeur faible par les équations \ref {eq:amplitude_a} et \ref {eq:amplitude_b}. Les courbes théoriques provenant de les amplitudes de probabilités de l'équation \ref {eq:expval_t_2} soit $\sqrt {|cos^2(\theta )|}$ pour $a$ et $\sqrt {|sin^2(\theta )|}$ pour $b$}{figure.caption.22}{}}
\@writefile{lof}{\contentsline {figure}{\numberline {23}{\ignorespaces Résultats de l'amplitude de probabilité pour le trajet de polarisation $\ket {D} \rightarrow \ket {R} \rightarrow \ket {A} \rightarrow \ket {L}$. Les données sont calculées à partir de la partie réelle de la valeur faible par les équations \ref {eq:amplitude_a} et \ref {eq:amplitude_b}. Les courbes théoriques provenant de les amplitudes de probabilités de l'équation \ref {eq:expval_t_3} soit $1/\sqrt  {2}$ pour $|a|$ et $|b|$.}}{88}{figure.caption.23}\protected@file@percent }
\newlabel{fig:DRAL_prob_amp}{{23}{88}{Résultats de l'amplitude de probabilité pour le trajet de polarisation $\ket {D} \rightarrow \ket {R} \rightarrow \ket {A} \rightarrow \ket {L}$. Les données sont calculées à partir de la partie réelle de la valeur faible par les équations \ref {eq:amplitude_a} et \ref {eq:amplitude_b}. Les courbes théoriques provenant de les amplitudes de probabilités de l'équation \ref {eq:expval_t_3} soit $1/\sqrt {2}$ pour $|a|$ et $|b|$}{figure.caption.23}{}}
